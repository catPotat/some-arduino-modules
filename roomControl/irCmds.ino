int fanLed =  13;
int ACLed = 11;
 
void irRemote() {
  // initialize the IR digital pin as an output:
  pinMode(fanLed, OUTPUT);      
  pinMode(ACLed, OUTPUT);      
}
 
void pulseIR(long microsecs, byte led) {
  cli();  // this turns off any background interrupts
 
  while (microsecs > 0) {
    // 38 kHz is about 13 microseconds high and 13 microseconds low
   digitalWrite(led, HIGH);  // this takes about 3 microseconds to happen
   delayMicroseconds(9);         // hang out for 10 microseconds, you can also change this to 9 if its not working
   digitalWrite(led, LOW);   // this also takes about 3 microseconds
   delayMicroseconds(9);         // hang out for 10 microseconds, you can also change this to 9 if its not working
 
   // so 26 microseconds altogether
   microsecs -= 26;
  }
 
  sei();  // this turns them back on
}

